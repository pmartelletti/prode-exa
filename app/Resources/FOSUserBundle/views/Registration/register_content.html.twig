<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register form-horizontal">
    <h3>Registrate para jugar al prode del Exa!</h3>
    <hr>
    {{ form_widget(form) }}
    <div class="form-actions">
        <input type="submit" class="btn btn-success" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
</form>